generator client {
  provider        = "prisma-client"
  previewFeatures = ["prismaSchemaFolder", "driverAdapters"]
  output          = "../generated"
  moduleFormat    = "esm"
  binaryTargets   = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
