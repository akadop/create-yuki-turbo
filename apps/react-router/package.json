{
  "name": "@yuki/react-router",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun with-env react-router build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "bun with-env react-router dev -p 3001",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "bun with-env react-router-serve ./build/server/index.js -p 3001",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.0.1",
    "@react-router/node": "^7.0.1",
    "@react-router/serve": "^7.0.1",
    "@t3-oss/env-core": "^0.11.1",
    "@tanstack/react-query": "^5.62.0",
    "@trpc/client": "^11.0.0-rc.648",
    "@trpc/react-query": "^11.0.0-rc.648",
    "@trpc/server": "^11.0.0-rc.648",
    "@yuki/api": "workspace:*",
    "@yuki/auth": "workspace:*",
    "@yuki/db": "workspace:*",
    "@yuki/ui": "workspace:*",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.1",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tailwind-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.16.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.2",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "prettier": "@yuki/prettier-config"
}
